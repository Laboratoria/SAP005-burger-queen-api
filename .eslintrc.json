{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
      "linebreak-style": ["error", "windows"],
      "object-curly-newline": ["error", {
        "ImportDeclaration": "never",
        "ExportDeclaration": "never"
        }]
    }
}
